@article {Desjardins-Proulx161125,
	author = {Desjardins-Proulx, Philippe and Poisot, Timoth{\'e}e and Gravel, Dominique},
	title = {Artificial Intelligence for Ecological and Evolutionary Synthesis},
	elocation-id = {161125},
	year = {2019},
	doi = {10.1101/161125},
	publisher = {Cold Spring Harbor Laboratory},
	abstract = {The grand ambition of theorists studying ecology and evolution is to discover the logical and mathematical rules driving the world{\textquoteright}s biodiversity at every level from genetic diversity within species to differences between populations, communities, and ecosystems. This ambition has been difficult to realize in great part because of the complexity of biodiversity. Theoretical work has led to a complex web of theories, each having non-obvious consequences for other theories. Case in point, the recent realization that genetic diversity involves a great deal of temporal and spatial stochasticity forces theoretical population genetics to consider abiotic and biotic factors generally reserved to ecosystem ecology. This interconnectedness may require theoretical scientists to adopt new techniques adapted to reason about large sets of theories. Mathematicians have solved this problem by using formal languages based on logic to manage theorems. However, theories ecology and evolution are not mathematical theorems, they involve uncertainty. Recent work in Artificial Intelligence in bridging logic and probability theory offers the opportunity to build rich knowledge bases that combine logic{\textquoteright}s ability to represent complex mathematical ideas with probability theory{\textquoteright}s ability to model uncertainty. We describe these hybrid languages and explore how they could be used to build a unified knowledge base of theories for ecology and evolution.},
	URL = {https://www.biorxiv.org/content/early/2019/08/28/161125},
	eprint = {https://www.biorxiv.org/content/early/2019/08/28/161125.full.pdf},
	journal = {bioRxiv}
}

